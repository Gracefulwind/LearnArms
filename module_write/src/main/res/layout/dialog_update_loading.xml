<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="446dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/shape_corner_12_white"
        >

        <TextView
            android:id="@+id/dul_tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="33dp"
            android:textSize="@dimen/public_font_16sp"
            android:textColor="#1F2B44"
            android:text="提交中，请等待"
            />

        <LinearLayout
            android:id="@+id/dul_ll_progress_container"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/dul_tv_title"
            android:layout_marginLeft="@dimen/public_dimen_35dp"
            android:layout_marginRight="@dimen/public_dimen_35dp"
            android:layout_marginTop="@dimen/public_dimen_10dp"
            android:orientation="vertical"
            >

            <ImageView
                android:id="@+id/dul_iv_indicator"
                android:layout_width="@dimen/public_dimen_32dp"
                android:layout_height="@dimen/public_dimen_32dp"
                android:scaleType="fitCenter"
                />

            <LinearLayout
                android:id="@+id/dul_ll_progress"
                android:layout_width="match_parent"
                android:layout_height="@dimen/public_dimen_30dp"
                android:background="@drawable/shape_corner_15_2878fa"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                >
                <ImageView
                    android:id="@+id/dul_iv_step1"
                    android:layout_width="@dimen/public_dimen_16dp"
                    android:layout_height="@dimen/public_dimen_16dp"
                    android:layout_marginLeft="@dimen/public_dimen_12dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/gif_uld_upload_ing"
                    />
                <ProgressBar
                    android:id="@+id/dul_pb_step1_2"
                    android:layout_width="@dimen/public_dimen_0dp"
                    android:layout_height="@dimen/public_dimen_4dp"
                    android:layout_weight="1"
                    style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                    android:max="100"
                    android:progress="0"
                    android:progressDrawable="@drawable/progressbar_update_loading_success"
                    />
                <ImageView
                    android:id="@+id/dul_iv_step2"
                    android:layout_width="@dimen/public_dimen_16dp"
                    android:layout_height="@dimen/public_dimen_16dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/gif_uld_upload_ing"
                    />
                <ProgressBar
                    android:id="@+id/dul_pb_step2_3"
                    android:layout_width="@dimen/public_dimen_0dp"
                    android:layout_height="@dimen/public_dimen_4dp"
                    android:layout_weight="1"
                    style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                    android:max="100"
                    android:progress="0"
                    android:progressDrawable="@drawable/progressbar_update_loading_success"
                    />
                <ImageView
                    android:id="@+id/dul_iv_step3"
                    android:layout_width="@dimen/public_dimen_16dp"
                    android:layout_height="@dimen/public_dimen_16dp"
                    android:layout_marginRight="@dimen/public_dimen_12dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/gif_uld_upload_ing"
                    />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/dul_ll_status"
            android:layout_width="@dimen/public_dimen_0dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/dul_ll_progress_container"
            android:layout_marginLeft="@dimen/public_dimen_35dp"
            android:layout_marginRight="@dimen/public_dimen_35dp"
            android:layout_marginTop="@dimen/public_dimen_12dp"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/public_color_1F2B44"
                android:textSize="@dimen/public_font_14sp"
                android:text="商机"
                />
            <androidx.legacy.widget.Space
                android:layout_width="@dimen/public_dimen_0dp"
                android:layout_height="@dimen/public_dimen_0dp"
                android:layout_weight="1"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/public_color_1F2B44"
                android:textSize="@dimen/public_font_14sp"
                android:text="预设问题"
                />
            <androidx.legacy.widget.Space
                android:layout_width="@dimen/public_dimen_0dp"
                android:layout_height="@dimen/public_dimen_0dp"
                android:layout_weight="1"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/public_color_1F2B44"
                android:textSize="@dimen/public_font_14sp"
                android:text="记事本"
                />

        </LinearLayout>

        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/dul_rl_error_buttons"
            android:layout_width="@dimen/public_dimen_0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/dul_ll_status"
            app:layout_constraintLeft_toLeftOf="@id/dul_ll_status"
            app:layout_constraintRight_toRightOf="@id/dul_ll_status"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/public_dimen_32dp"
            android:layout_marginBottom="@dimen/public_dimen_32dp"
            >

            <TextView
                android:id="@+id/dul_tv_cancel"
                android:layout_width="@dimen/public_dimen_172dp"
                android:layout_height="@dimen/public_dimen_48dp"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_button_cancel"
                android:gravity="center"
                android:textSize="@dimen/public_font_14sp"
                android:textColor="#2A7AFF"
                android:text="取消"
                />

            <TextView
                android:id="@+id/dul_tv_retry"
                android:layout_width="@dimen/public_dimen_172dp"
                android:layout_height="@dimen/public_dimen_48dp"
              android:layout_alignParentRight="true"
                android:background="@drawable/shape_button_confirm"
                android:gravity="center"
                android:textSize="@dimen/public_font_14sp"
                android:textColor="#FFFFFF"
                android:text="重新提交"
                />
        </RelativeLayout>

        <androidx.legacy.widget.Space
            android:layout_width="@dimen/public_dimen_0dp"
            android:layout_height="@dimen/public_dimen_0dp"
            app:layout_constraintTop_toBottomOf="@id/dul_ll_status"
            android:layout_marginTop="@dimen/public_dimen_32dp"
            app:layout_constraintLeft_toLeftOf="parent"
            />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>